    "# get the unique_person list\n",
    "person_ids = transcript_offer.person.unique()\n",
    "\n",
    "# record the time\n",
    "start = time()\n",
    "for person_id in person_ids:\n",
    "    # transcript for unique_person\n",
    "    is_unique_person = (transcript_offer['person']==person_id)\n",
    "    unique_person = transcript_offer[is_unique_person]\n",
    "    \n",
    "    # all transactions of unique_person\n",
    "    is_transaction = (unique_person.event=='transaction')\n",
    "    transactions = unique_person[is_transaction]\n",
    "    \n",
    "    # get groupby object according to 'offer_id' of a unique_person\n",
    "    offer_id_Groupby = unique_person.groupby(['offer_id'])\n",
    "    \n",
    "    # get list of offer_ids of a unique_person\n",
    "    offer_id_list = unique_person.offer_id.unique()\n",
    "    \n",
    "    # update target_data_list and transcript_offer\n",
    "    data_process.construct_data_from_unique_offer_id(offer_id_Groupby, offer_id_list, transactions)\n",
    "end = time()\n",
    "print(\"Data for all person wrangled, time cost: {}s\" .format(end - start))"
   ]
