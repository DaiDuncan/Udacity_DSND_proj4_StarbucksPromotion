    "# create dict: {\"original Code\": int number}\n",
    "id_mapping = defaultdict(int)\n",
    "\n",
    "# original codes of ids\n",
    "id_codes = profile['id'].unique().tolist()\n",
    "\n",
    "num = 0\n",
    "for id_code in id_codes:\n",
    "    id_mapping[id_code] = num\n",
    "    num += 1\n",
    "    \n",
    "def map_id_2_int(df, map_col, map_dict=id_mapping):\n",
    "    '''Map 'id' from encode to int \n",
    "    \n",
    "    INPUT:\n",
    "    - df(DataFrame): the target DataFrame in which 'id' to be mapped\n",
    "    - map_col(str): the target col ('id') to be mapped\n",
    "    - map_dict(dict): mapping dict {\"original Code\": int number}\n",
    "    \n",
    "    OUTPUT:\n",
    "    - df(DataFrame): already mapped DataFrame\n",
    "    '''\n",
    "    df[map_col] = df[map_col].apply(lambda x: id_mapping[x])\n",
    "    \n",
    "    return df\n",
    "\n",
    "# map 'id' in profile from encodes to int\n",
    "profile_2_clean = map_id_2_int(profile, 'id')\n",
    "profile_2_clean"
   ]
