    "    for segment_df in segment_dfs:\n",
    "        is_group = (segment_df.label_effective_offer == 0) & (segment_df.amount_with_offer > 0) & (segment_df.time_viewed.notnull())\n",
    "        segment_grouped.append(segment_df[is_group])\n",
    "    \n",
    "    return segment_grouped"
   ]
  },
